# App Configuration
VITE_APP_NAME=Shadcn Admin
VITE_APP_URL=http://localhost:5173

# API Configuration
VITE_API_URL=http://localhost:3000
VITE_API_TIMEOUT=5000

# Authentication
VITE_AUTH_SECRET=your-auth-secret-key
VITE_AUTH_DOMAIN=your-auth-domain
VITE_AUTH_REDIRECT_URI=http://localhost:5173/auth/callback

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_DARK_MODE=true

# Development
VITE_DEV_SERVER_PORT=5173
VITE_DEV_SERVER_HOST=localhost

# Production
VITE_PRODUCTION_URL=https://your-production-url.com

# API Keys (replace with your actual keys)
VITE_API_KEY=your-api-key
VITE_STRIPE_PUBLIC_KEY=your-stripe-public-key

# Database (if needed)
VITE_DATABASE_URL=your-database-url

# Cache Configuration
VITE_CACHE_STALE_TIME=10000  # 10 seconds in milliseconds
VITE_CACHE_RETRY_COUNT=3

# Error Handling
VITE_ERROR_REPORTING_ENABLED=false
VITE_ERROR_REPORTING_URL=your-error-reporting-url